<ul class="mx-0 my-3 p-0">
{%- for post in postslist | reverse %}
	<li class="py-2 d-flex justify-content-between align-items-center">
		<a href="{{ post.url }}" class="text-body">{% if post.data.title %}{{ post.data.title }}{% else %}<code>{{ post.url }}</code>{% endif %}</a>
		<time datetime="{{ post.date | htmlDateString }}">{{ post.date | readableDate("dd LLLL yyyy") }}</time>
	</li>
{%- endfor %}
</ul>
